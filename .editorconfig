# Editor configuration, see https://editorconfig.org
root = false

[*]
charset = utf-8
indent_style = tab
insert_final_newline = true
trim_trailing_whitespace = true
quote_type = double

[*.py]
indent_size = 4

[*.{h,cpp}]
indent_size = 4

[*.md]
max_line_length = off
trim_trailing_whitespace = false
